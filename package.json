{
  "name": "start",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel security-check run:src lintjs:watch lintstyles:watch",
    "run:src": "nodemon scripts/srcServer.js --exec babel-node --ignore src/",
    "lintjs": "esw 'src/**/*.js'",
    "lintjs:watch": "npm run lintjs -- --watch",
    "prelintstyles": "stylefmt --config .stylelintrc --recursive 'src/**/*.css'",
    "lintstyles": "stylelint 'src/**/*.css'",
    "lintstyles:watch": "npm run lintstyles -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "babel-node node_modules/.bin/ospec",
    "test:watch": "npm run test -- --watch",
    "cover": "NODE_ENV=test nyc --no-cache ospec",
    "check-coverage": "NODE_ENV=test nyc check-coverage --branches -9",
    "generate-mock-data": "babel-node scripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/data/db.json --port 3002",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lintjs",
    "build": "webpack -p --config scripts/webpack.config.prod.js",
    "start-dist-server": "babel-node scripts/distServer.js",
    "postbuild": "NODE_ENV=production npm-run-all --parallel start-dist-server",
    "deploy": "surge ./dist"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://dontwork@github.com/dontwork/start.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dontwork/start/issues"
  },
  "homepage": "https://github.com/dontwork/start#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-dynamic-import-node": "^1.0.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.23.0",
    "babili-webpack-plugin": "0.0.10",
    "browser-sync": "^2.18.8",
    "bs-fullscreen-message": "^1.1.0",
    "chalk": "^1.1.3",
    "colors.css": "^3.0.0",
    "compression": "^1.6.2",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "eslint": "^3.16.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-watch": "^3.0.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.28.0",
    "isomorphic-ensure": "^1.0.1",
    "isomorphic-style-loader": "^1.1.0",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.5",
    "localtunnel": "^1.8.2",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "nyc": "^10.1.2",
    "ospec": "^1.2.3",
    "redux-immutable-state-invariant": "^1.2.4",
    "reload-server-webpack-plugin": "^1.0.1",
    "resource-hints-webpack-plugin": "0.0.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.2",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "stylefmt": "^5.1.2",
    "stylelint": "^7.9.0",
    "stylelint-config-css-modules": "^0.1.0",
    "stylelint-config-standard": "16.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-md5-hash": "^0.0.5"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "async": "^2.1.5",
    "axios": "^0.15.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "breakpoint-sass": "^2.7.1",
    "compass": "^0.1.1",
    "formidable": "^1.1.1",
    "marked": "^0.3.6",
    "mithril": "^1.0.1",
    "mithril-node-render": "^2.0.0",
    "moment": "^2.17.1",
    "mongoose": "^4.8.4",
    "mongoose-auto-increment": "^5.0.1",
    "multer": "^1.3.0",
    "ramda": "^0.23.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reset-css": "^2.2.0",
    "susy": "^2.2.12",
    "synchronize": "^2.0.0",
    "unirest": "^0.5.1",
    "validate.js": "^0.11.1",
    "w3c-xmlhttprequest": "^2.1.2"
  }
}
